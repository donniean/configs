{
  "name": "@donniean/configs",
  "version": "6.4.0",
  "description": "an opinionated configs generator",
  "keywords": [
    "cli",
    "commitizen",
    "commitlint",
    "cspell",
    "editorconfig",
    "eslint",
    "gitattributes",
    "gitignore",
    "htmlhint",
    "husky",
    "lint-staged",
    "markdownlint",
    "prettier",
    "sort-package-json",
    "stylelint",
    "tsc"
  ],
  "homepage": "https://github.com/donniean/configs#readme",
  "bugs": {
    "url": "https://github.com/donniean/configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/donniean/configs.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "configs": "./bin/configs.js"
  },
  "files": [
    "assets",
    "bin",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc && tsc-alias",
    "build:watch": "npm run clean && tsc --watch & tsc-alias --watch",
    "clean": "rimraf ./dist/",
    "commit": "cz",
    "lint:cspell": "cspell lint --no-progress --relative --no-must-find-files --dot --gitignore \"**\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts}\"",
    "lint:eslint:fix": "eslint --fix \"**/*.{js,mjs,cjs,ts}\"",
    "lint:markdownlint": "markdownlint --dot \"**/*.md\"",
    "lint:markdownlint:fix": "markdownlint --dot --fix \"**/*.md\"",
    "lint:prettier": "prettier --check --ignore-unknown \"**\"",
    "lint:prettier:fix": "prettier --write --ignore-unknown \"**\"",
    "lint:sort-package-json": "sort-package-json --check",
    "lint:sort-package-json:fix": "sort-package-json",
    "lint:tsc": "tsc --noEmit",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "clean-deep": "^3.4.0",
    "commander": "^11.0.0",
    "cosmiconfig": "^8.2.0",
    "date-fns": "^2.30.0",
    "deepmerge": "^4.3.1",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "fs-extra": "^11.1.1",
    "javascript-stringify": "^2.1.0",
    "latest-version": "^7.0.0",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.5",
    "nanoid": "^4.0.2",
    "ora": "^6.3.1",
    "parse-glob": "^3.0.4",
    "prettier": "^3.0.0",
    "prompts": "^2.4.2",
    "shelljs": "^0.8.5",
    "sort-object-keys": "^1.1.3",
    "sort-package-json": "^2.5.0",
    "stylelint": "^15.10.1",
    "type-fest": "^3.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node-lts": "^18.12.3",
    "@types/eslint": "^8.44.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.7",
    "@types/micromatch": "^4.0.2",
    "@types/parse-glob": "^3.0.29",
    "@types/prompts": "^2.4.4",
    "@types/shelljs": "^0.8.12",
    "@types/sort-object-keys": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitest/coverage-c8": "^0.33.0",
    "commitizen": "^4.3.0",
    "cspell": "^6.31.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vitest": "^0.2.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "markdownlint-cli": "^0.35.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.7",
    "sort-package-json": "^2.5.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
