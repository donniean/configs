{
  "name": "@donniean/configs",
  "version": "4.0.0",
  "description": "",
  "keywords": [
    "cli",
    "commitizen",
    "commitlint",
    "cspell",
    "editorconfig",
    "eslint",
    "gitattributes",
    "gitignore",
    "htmlhint",
    "husky",
    "lint-staged",
    "prettier",
    "stylelint",
    "tsc"
  ],
  "homepage": "https://github.com/donniean/configs#readme",
  "bugs": {
    "url": "https://github.com/donniean/configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/configs.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "main": "lib/index.js",
  "bin": {
    "configs": "./bin/configs.js"
  },
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "build": "tsc --project tsconfig.build.json",
    "lint:format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "lint:typescript": "tsc --noEmit",
    "lint:script": "eslint --fix \"**/*.{js,ts}\"",
    "lint:spell": "cspell \"**\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "release": "standard-version",
    "pub": "npm run release &&  npm publish --access public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "cosmiconfig": "^7.0.1",
    "fs-extra": "^10.0.0",
    "got": "^11.8.3",
    "inquirer": "^8.2.0",
    "latest-version": "^6.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ora": "^6.0.1",
    "prettier": "^2.5.0",
    "read-pkg": "^7.0.0",
    "sort-object-keys": "^1.1.3",
    "sort-package-json": "^1.53.1",
    "webpack-merge": "^5.8.0",
    "write-pkg": "^5.0.0",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.177",
    "@types/prettier": "^2.4.2",
    "@types/sort-object-keys": "^1.1.0",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "commitizen": "^4.2.4",
    "cspell": "^5.13.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  }
}
